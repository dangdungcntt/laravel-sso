{
    "name": "nddcoder/laravel-sso",
    "description": "Simple Laravel SSO integration (Based on zefy/laravel-sso)",
    "keywords": ["laravel", "sso", "authentication", "login"],
    "homepage": "https://github.com/dangdungcntt/laravel-sso",
    "license": "MIT",
    "authors": [
      {
        "name": "Dung Nguyen Dang",
        "email": "dangdungcntt@gmail.com",
        "homepage": "https://github.com/dangdungcntt",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^5.5",
        "zefy/php-simple-sso": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "Nddcoder\\LaravelSSO\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "Nddcoder\\LaravelSSO\\SSOServiceProvider"
        ]
      }
    }
}